    "exceptions",
    "handling",
    "error",
    "callback"
  ],
  "author": {
    "name": "Mario Pareja",
    "email": "pareja.mario@gmail.com",
    "url": "http://www.mariopareja.com/blog"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tap": "~0.4.1",
    "jslint": "~0.1.9"
  }
